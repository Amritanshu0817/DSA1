
  vector<int> leaders(int a[], int n){
        // Code here
        vector<int> element;
        for(int i=0; i<n; i++){
            for(int j=1; j<n; j++){
                 if(a[i] <= a[j])
                 break;
                 
                 element.push_back(a[i]);
            }
         //  element.push_back(a[i]);
        }
        return element;
    }
