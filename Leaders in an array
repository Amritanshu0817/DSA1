
class Solution{
    //Function to find the leaders in the array.
    public:
    vector<int> leaders(int a[], int n){
        // Code here
        int i=0;
        vector<int> l;
        for(; i<n; i++){
            for(int j=1; j<n; j++){
                if(a[i] < a[j]){
                    break;
                }
            }
             l = a[i];
        }
        return l;
    }
};
