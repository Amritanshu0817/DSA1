class Solution{
  public:
    // Function to find floor of x
    // n: size of vector
    // x: element whose floor is to find
    int findFloor(vector<long long> v, long long n, long long x){
        
        // Your code here
         int cnt = 0;
    for(int i=0; i<n; i++){
        if(v[i] <= x)
        cnt++;
    }
   return(cnt-1);
        
    }
};

